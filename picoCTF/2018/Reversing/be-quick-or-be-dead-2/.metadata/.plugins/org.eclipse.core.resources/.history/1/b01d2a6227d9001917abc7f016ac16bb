package fib;

public class fib {
	static int[] fibs;
	static int NUM_FIB = 48;
	
	public static void main(String[] args) {
		fibs = new int[NUM_FIB + 1];
		fibs[1] = 0;
		fibs[2] = 1;
		for (int i = 3; i < NUM_FIB + 1; i++) {
			fibs[i] = fibs[i-1] + fibs[i-2];
		}
		System.out.println(fibs[NUM_FIB]);
	}
}
